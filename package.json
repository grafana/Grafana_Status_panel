{
  "name": "vonage-status-panel",
  "private": true,
  "version": "2.0.4",
  "description": "Status Panel for Grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix && prettier --write --list-different .",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vonage/Grafana_Status_panel.git"
  },
  "author": "Vonage",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/plugin-e2e": "^2.0.6",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "^1.52.0",
    "@swc/core": "1.13.5",
    "@swc/helpers": "0.5.17",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "14.3.1",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "@types/react-router-dom": "^5.2.0",
    "@types/testing-library__jest-dom": "5.14.9",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lefthook": "^1.6.12",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.93.2",
    "sass-loader": "13.3.3",
    "style-loader": "3.3.4",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.9.3",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "imports-loader": "^5.0.0",
    "semver": "^7.6.3",
    "terser-webpack-plugin": "^5.3.10",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2",
    "@stylistic/eslint-plugin-ts": "^2.9.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsdoc": "^46.8.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@emotion/css": "11.13.5",
    "@grafana/data": "^12.0.2",
    "@grafana/runtime": "^12.0.2",
    "@grafana/schema": "^12.0.2",
    "@grafana/ui": "^12.0.2",
    "react": "18.2.0",
    "react-card-flip": "^1.2.2",
    "react-dom": "18.2.0",
    "tslib": "2.8.1"
  }
